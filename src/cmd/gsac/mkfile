< $GLOBALSCRIPT/src/mkhdr

MKSHELL=$PLAN9/bin/rc

GSLIB_TARGETS=`{$PLAN9/bin/rc ./gslib-targets}

<| $PLAN9/bin/rc ./gslib-deps -l $GLOBALSCRIPT/gslib $GSLIB_TARGETS

interpreted:V:
	true

test-interpreted:V: interpreted
	test-gslib ./gslib

integration-test-interpreted:V: interpreted test-interpreted
	./rc/test-interpreted

install-interpreted:V: interpreted test-interpreted $GLOBALSCRIPT/bin/gsac $GLOBALSCRIPT/bin/gsac-targets $GLOBALSCRIPT/bin/gsac-deps

$GLOBALSCRIPT/bin/gsac: gsac.rc interpreted test-interpreted
	cp gsac.rc $GLOBALSCRIPT/bin/gsac
	chmod a+x $GLOBALSCRIPT/bin/gsac

$GLOBALSCRIPT/bin/gsac-targets: gsac-targets.rc interpreted test-interpreted
	cp gsac-targets.rc $GLOBALSCRIPT/bin/gsac-targets
	chmod a+x $GLOBALSCRIPT/bin/gsac-targets

$GLOBALSCRIPT/bin/gsac-deps: gsac-deps.rc interpreted test-interpreted
	cp gsac-deps.rc $GLOBALSCRIPT/bin/gsac-deps
	chmod a+x $GLOBALSCRIPT/bin/gsac-deps

effect:V: $GSLIB_TARGETS
	true
