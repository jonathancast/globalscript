#! /usr/local/plan9/bin/rc

# §paragraph{C Testing library}
cd $GLOBALSCRIPT/src/libtest &&
    mk install &&
# §paragraph{IBIO & Its Pre-requisites}
    cd ../libglobalscript/ &&
    mk install &&
    cd ../cmd &&
    mk pure-install &&
    cd ibio/ &&
    mk install &&
# §paragraph{Interpreted Compiler}
    cd ../gsac &&
    mk interpreted && mk install-interpreted && mk effect
